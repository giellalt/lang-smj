SET NOT-NPMOD-NOMPL     = WORD - PRE-NP-HEAD - (Nom Pl) OR ABBR OR @CVP  OR (Adv Subqst) ;

SET NOT-NPMOD-ACC     = NOT-NPMOD - Acc OR ABBR  OR (Adv Subqst);
SET NOT-NPMOD-ACC-ADV     = NOT-NPMOD - Acc - Adv OR ABBR  OR (Adv Subqst);

SET NOT-NPMODADV    = WORD - PRE-NP-HEAD - Adv  OR (Adv Subqst) ;
SET NOT-NPMODADV-INDEF   = WORD - PRE-NP-HEAD - Adv - Indef  OR (Adv Subqst) ;
SET NOT-NPMODADVPCLE = WORD - PRE-NP-HEAD - Adv - Pcle OR (Adv Subqst) ;


