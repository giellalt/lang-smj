        +Der5+Der/AAdv+Adv:a%>Q1t ENDLEX         ;
                +Cmp/SgNom:%>     R              ;
                +Cmp/SgGen:a%>Q1  R              ;
                +Cmp/PlGen:a%>Q1j R              ;
