                    +A:%>X4  ILL-EVENCASEadj   ;
                    +A:%>Q1  N-EVENWEAKSTEMadj ; ! Presumably WeG in nominal paradigm
                    +A:%>Q1  BU/MUS            ;
  +A+Der5+Der/AAdv+Adv:%>Q1t ENDLEX            ;
                    +A:%>    MUORRACmp         ;

