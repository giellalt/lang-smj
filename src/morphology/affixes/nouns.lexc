                                    +N+Pl+Gen:%>Q1j          K        ;
                                    +N+Pl+Acc:%>Q1jt         K        ;
                                    +N+Pl+Ill:%>Q1jda        K        ;
                                    +N+Pl+Ela:%>Q1js         K        ;
                                    +N+Pl+Ine:%>Q1jn         K        ;
                                    +N+Pl+Com:%>Q1j          K        ;
                                    +N+Pl+Nom:%>Q1%>         PxNomPlV ; !
                                    +N+Pl+Gen:%>Q1j%>        PxC      ; !
                                    +N+Pl+Acc:%>Q1jd%>       PxC      ; !
                                    +N+Pl+Ill:%>Q1jdis%>     PxCs     ; !
                                    +N+Pl+Ine:%>Q1jn%>       PxC      ; !
                                    +N+Pl+Ela:%>Q1jst%>      PxC      ; !
                                    +N+Pl+Com:%>Q1j%>        PxC      ; !
                                       +N+Ess:%>n            K        ;
                                       +N+Abe:%>Q1dak        K        ;
                                       +N+Abe:%>Q1da         K        ;
                                    +N+Pl+Abe:%>Q1j#dagi     K        ;
                                    +N+Pl+Abe:%>Q1j#dagá     K        ;
                           +Err/Orth+N+Pl+Abe:%>Q1j#tagá     K        ;
                            +N+Der1+Der/Car+A:»Q1            DIBME    ;
                       +N+Der1+Der/Car+A+Attr:»Q1dis%>       ATTR     ;
                            +N+Der1+Der/Car+A:»Q1d           DUVVAT   ;
