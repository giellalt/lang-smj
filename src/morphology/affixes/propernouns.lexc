LEXICON ACCRA-DC
         +Sg+Nom:      K     ;
         +Sg+Nom:      RHyph ;
            +Ess:n     K     ;
         +Sg+Ill:Q4j   K     ; !Q4 to get e:i
         +Sg+Gen:      K     ;
         +Sg+Gen:      RHyph ;
         +Sg+Acc:v     K     ;
         +Sg+Ine:n     K     ;
         +Sg+Ela:s     K     ;
         +Pl+Nom:      K     ;
         +Pl+Gen:Q4j   K     ; !Q4 to get e:i
         +Pl+Gen:Q4j   K     ; !Q4 to get e:i
         +Pl+Acc:Q4jt  K     ;
         +Pl+Ill:Q4jda K     ;
         +Pl+Ine:Q4jn  K     ;
         +Pl+Ela:Q4js  K     ;
         +Pl+Com:Q4j   K     ; !Q4 to get e:i
         +Sg+Com:Q4jn  K     ;
 +Sg+Com+Err/Sub:X5jn  K     ; ! Norgejn

LEXICON ACCRA-DC-ORG
         +Sg+Nom:     K     ;
         +Sg+Nom:     RHyph ;
            +Ess:n    K     ;
         +Sg+Ill:Q4j  K     ; !Q4 to get e:i
         +Sg+Gen:     K     ;
         +Sg+Gen:     RHyph ;
         +Sg+Acc:v    K     ;
         +Sg+Ine:n    K     ;
         +Sg+Ela:s    K     ;
! +Pl+Nom: K ;
! +Pl+Gen:Q4j K ; !Q4 to get e:i
! +Pl+Gen:Q4j K ; !Q4 to get e:i
! +Pl+Acc:Q4jt K ;
! +Pl+Ill:Q4jda K ;
! +Pl+Ine:Q4jn K ;
! +Pl+Ela:Q4js K ;
! +Pl+Com:Q4j K ; !Q4 to get e:i
         +Sg+Com:Q4jn K     ;
 +Sg+Com+Err/Sub:X5jn K     ; ! Norgejn


LEXICON ACCRA-DC-LOAN
         +Sg+Nom:     K     ;
         +Sg+Nom:     RHyph ;
    +Ess+Err/Sub:n    K     ; !
 +Sg+Ill+Err/Sub:Q4j  K     ; ! !Q4 to get e:i
 +Sg+Gen+Err/Sub:     K     ; !
 +Sg+Gen+Err/Sub:     RHyph ; !
 +Sg+Acc+Err/Sub:v    K     ; !
 +Sg+Ine+Err/Sub:n    K     ; !
 +Sg+Ela+Err/Sub:s    K     ; !
! +Pl+Nom+Err/Sub: K ; !
! +Pl+Gen+Err/Sub:Q4j K ; ! !Q4 to get e:i
! +Pl+Gen+Err/Sub:Q4j K ; ! !Q4 to get e:i
! +Pl+Acc+Err/Sub:Q4jt K ; !
! +Pl+Ill+Err/Sub:Q4jda K ; !
! +Pl+Ine+Err/Sub:Q4jn K ; !
! +Pl+Ela+Err/Sub:Q4js K ; !
! +Pl+Com+Err/Sub:Q4j K ; ! !Q4 to get e:i
 +Sg+Com+Err/Sub:Q4jn K     ; !
 +Sg+Com+Err/Sub:X5jn K     ; ! !SUB Norgejn


