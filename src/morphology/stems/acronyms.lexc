!LEXICON quasiacro moved to HyphNouns
!CD-rom CELSIUS ;
!CD-singel GAHPER ; !^LOAN
!P2-kanálla:P2-kan AREAL ;
!cd-rom CELSIUS ;


LEXICON ACRO_vow  !!= @CODE@ Acronyms ending in vowels and which are pronounced as a word, like NATO and UNESCO (Giellagálldo bievddegirjje 2/2016). These only get the case endings: NATO:v, NATO:n, etc. 
+N+ACR: acroconnector_vow  ;

LEXICON acroconnector_vow ! Here comes a set of possible symbols to put between the abbreviation and its suffix.
                acronull_vow  ;
          :%>%: acrosuff_vow  ; 
                RHyph         ; ! Acro compounds: CD-2
          
 +Err/Orth:     acrosuff_vow  ; ! The lexica are split in order to avoid the POS
 +Err/Orth:%>'7 acrosuff_vow  ; ! tag for other symbols using the acroconnector
 +Err/Orth:%>%` acrosuff_vow  ; ! lexicon. (cf. the punct-sme-lex.txt file)
 +Err/Orth:%>%´ acrosuff_vow  ;
 +Err/Orth:     R             ; ! Without hyphen, not normative:  NRK23

LEXICON acronull_vow 
         +Sg+Nom:%> # ;
         +Sg+Gen:%> # ;
         +Pl+Nom:%> # ;


LEXICON acrosuff_vow 
			acrosuff ;
+Err/Orth:a acrosuff ;

LEXICON acrosuff
 +Sg+Acc:v            # ;
 +Sg+Ill:j            # ;
 +Sg+Ine:n            # ; ! NRK:n
 +Sg+Ela:s            # ; ! NRK:s
 +Sg+Com:jn           # ;
     +Ess:n           # ; ! NBR:n
 +Pl+Gen:j            # ;
 +Pl+Acc:jt           # ;
 +Pl+Ill:jda          # ;
 +Pl+Ine:jn           # ;
 +Pl+Ela:js           # ;
 +Pl+Com:j            # ;
 

LEXICON ACRO_cons  !!= @CODE@ Mostly acronyms ending in consonants, but also acronyms ending in vowels which are not pronounced as words, like LO and CV (Giellagálldo bievddegirjje 2/2016). These get case endings like a regular a-stem noun with both the vowel and case ending: LO:av, LO:an, etc. 
+N+ACR: acroconnector_cons ;

LEXICON acroconnector_cons ! Here comes a set of possible symbols to put between the abbreviation and its suffix. 
                acronull_cons  ;
          :%>%: acrosuff_cons  ; 
                RHyph          ; ! Acro compounds: CD-2
          
 +Err/Orth:     acrosuff_cons  ; ! The lexica are split in order to avoid the POS
 +Err/Orth:%>'7 acrosuff_cons  ; ! tag for other symbols using the acroconnector
 +Err/Orth:%>%` acrosuff_cons  ; ! lexicon. (cf. the punct-sme-lex.txt file)
 +Err/Orth:%>%´ acrosuff_cons  ;
 +Err/Orth:     R                  ; ! Without hyphen, not normative:  NRK23


LEXICON acronull_cons
         +Sg+Nom:%> # ;
         +Sg+Gen:%>%:a # ;
         +Pl+Nom:%>%:a # ;
+Sg+Gen+Err/Orth:%> # ;
+Pl+Nom+Err/Orth:%> # ;

LEXICON acrosuff_cons
+Err/Orth:  acrosuff ;
         :a acrosuff ;


