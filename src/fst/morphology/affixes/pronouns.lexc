! *Personal 
! *Demonstrative 
! *Reflexive 
! *Reciprocal 
! *Interrogative 
! *Relative 
! *Indefinite 


! Personal pronoun
! -----------------

LEXICON persproncasesg
                    +Nom:å%>n      K-CONS ;
             +Use/NG+Nom:å%>nnå    K-VOW ; !betont
    +Err/Orth+Use/NG+Nom:å%>nna    K-VOW ; !betont

                    +Gen:u%>v      K-CONS ;
!             +Use/NG+Gen:u%>v      NAMÁK ; !why does it go here (see stems>adjec)? adjectival -k derivation does not take pronouns
             +Use/NG+Gen:u%>vva    K-VOW ; !!betont
                    +Acc:u%>v      K-CONS ;
             +Use/NG+Acc:u%>vva    K-VOW ; !!betont
                    +Ill:u%>nji    K-VOW ;
  +Use/NG+Use/-Spell+Ill:u%>nje    K-VOW ; !dial
           +Err/Orth+Ill:u%>jni    K-VOW ;
                    +Ine:u%>jna    K-VOW ;
  +Use/NG+Use/-Spell+Ine:u%>ne     K-VOW ; !dial
  +Use/NG+Use/-Spell+Ine:u%>jne    K-VOW ; !dial
                    +Ela:u%>jsta   K-VOW ;
  +Use/NG+Use/-Spell+Ela:u%>jste   K-VOW ; !dial
  +Use/NG+Use/-Spell+Ela:u%>sta    K-VOW ; !dial
  +Use/NG+Use/-Spell+Ela:u%>ste    K-VOW ; !dial
                    +Par:u%>nnut   K-CONS ;
                    +Com:u%>jna    K-VOW ;
  +Use/NG+Use/-Spell+Com:u%>jne    K-VOW ; !dial
                    +Abe:u%>v#dá   K-VOW ;
                    +Abe:u%>v#da   K-VOW ;
            +Err/Orth+Abe:u%>v#tá   K-VOW ;
             +Use/NG+Abe:u%>v#dak  K-CONS ; !
             +Use/NG+Abe:u%>v#dagi K-VOW ; !
             +Use/NG+Abe:u%>v#dagá K-VOW ; !
            +Err/Orth+Abe:u%>v#tagá K-VOW ; !
            +Err/Orth+Abe:u%>#dá    K-VOW ; !
            +Err/Orth+Abe:u%>#da    K-VOW ; !
            +Err/Orth+Abe:u%>#dak   K-CONS ; !
