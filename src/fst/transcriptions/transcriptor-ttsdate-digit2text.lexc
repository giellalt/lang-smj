         @U.MONTH.6@:@U.MONTH.6@biehtsemáno%     30DAYS ;
  @U.MONTH.6@+Use/NG:@U.MONTH.6@Biehtsemáno%     30DAYS ;

         @U.MONTH.7@:@U.MONTH.7@snjilltjamáno%   31DAYS ;
  @U.MONTH.7@+Use/NG:@U.MONTH.7@sjnjilltjamáno%  31DAYS ;
  @U.MONTH.7@+Use/NG:@U.MONTH.7@Snjilltjamáno%   31DAYS ;
  @U.MONTH.7@+Use/NG:@U.MONTH.7@Sjnjilltjamáno%  31DAYS ;

         @U.MONTH.8@:@U.MONTH.8@bårggemáno%      31DAYS ;
