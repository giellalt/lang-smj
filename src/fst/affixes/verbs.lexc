 :e%>   I-du83pl72 ;
 :%>    I-du7pl9   ;

LEXICON II-even-V
 :Y1 II-sg72pl9 ; ! Y1 deletes stem vowels in front of j
 :Q1 II-other   ; ! Q1, WeG. Q1 deletes stem vow bef. j
! Y1 does not give WeG as Q1 does, therefore the split

LEXICON I-o-V
 :Q2 I-sg72      ; ! Q2 governs WeG and e:á
 :Y2 I-sg9       ; ! Y2 gives á in sg9 of verbs
     I-du83pl72  ; ! No diacritic, no stem changes
 :Y1 I-du7pl9-o- ; ! Y1 deletes stem vowels in front of j

LEXICON II-o-V
 :Y1 II-sg72pl9-o- ; ! Y1 deletes stem vowels in front of j
 :Q1 II-other      ; ! Q1, WeG. Q1 deletes stem vow bef. j
! Y1 does not give WeG as Q1 does, therefore the split

LEXICON II-odd-V
    II-sg72pl9 ;
 :i II-other   ;

LEXICON I-contr-V
    I-sg72     ;
    I-sg9      ;
    I-du83pl72 ;
 :j I-du7pl9   ;

LEXICON II-contr-V
 :j II-sg72pl9 ;
    II-other   ; !w/o j, since j in target lexicon



