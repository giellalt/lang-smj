        +IV+Pot:%>Q1tj    I-odd-V            ;
        +IV+Inf:%>t       K                  ;
     +IV+PrfPrc:%>Y3m     K                  ;
  +IV+Actio+Nom:m         K                  ;
  +IV+Actio+Gen:ma        K                  ;
  +IV+Actio+Com:mijn      K                  ;
  +IV+Actio+Ess:min       K                  ;
  +IV+Actio+Ela:mis       K                  ;

 +IV+Ind+ConNeg:%>Q1      K                  ;
 +IV+Pot+ConNeg:%>Q1tja   K                  ;

       +IV+Cond:%>»Q2lulu V-ODD-PRET         ; ! gives galgaluluv osv is an old form that is seldom used today. it is not mentioned in other grammars than briefly in Spiik and more thoroughly in Grundström
   @C.NeedNoun@           GALGGAT_IV_flagged ;

LEXICON GALGGAT_IV_flagged
