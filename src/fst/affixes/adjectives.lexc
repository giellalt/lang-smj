                    :%>    NE-EVENCASEadj    ;
                    :%>X4  ILL-EVENCASEadj   ;
                    :%>Q1  N-EVENWEAKSTEMadj ;
                    :%>Q1  BU/MUS            ;
  +Der5+Der/AAdv+Adv:%>Q1t K-CONS            ;
                    :%>    MUORRACmp         ;

