LEXICON ACCRA-DC
            +Attr:      K-VOW  ;
          +Sg+Nom:      K-VOW  ;
       +Cmp/SgNom:      RHyph  ;
             +Ess:n     K-CONS ;
          +Sg+Ill:Q4j   K-CONS ; !Q4 to get e:i
 +Sg+Ill+Err/Orth:%:j   K-CONS ; !Q4 to get e:i
          +Sg+Gen:      K-VOW  ;
       +Cmp/SgGen:      RHyph  ;
          +Sg+Acc:v     K-CONS ;
          +Sg+Ine:n     K-CONS ;
          +Sg+Ela:s     K-CONS ;
 +Sg+Acc+Err/Orth:%:v   K-CONS ;
 +Sg+Ine+Err/Orth:%:n   K-CONS ;
 +Sg+Ela+Err/Orth:%:s   K-CONS ;
          +Pl+Nom:      K-VOW  ;
          +Pl+Gen:Q4j   K-CONS ; !Q4 to get e:i
          +Pl+Gen:Q4j   K-CONS ; !Q4 to get e:i
          +Pl+Acc:Q4jt  K-CONS ;
          +Pl+Ill:Q4jda K-VOW  ;
          +Pl+Ine:Q4jn  K-CONS ;
          +Pl+Ela:Q4js  K-CONS ;
          +Pl+Com:Q4j   K-CONS ; !Q4 to get e:i
          +Sg+Com:Q4jn  K-CONS ;
 +Sg+Com+Err/Orth:X5jn  K-CONS ; ! Norgejn
 +Sg+Com+Err/Orth:%:jn  K-CONS ; ! Norgejn

 

LEXICON ACCRA-DC-ORG
          +Sg+Nom:      K-VOW  ;
       +Cmp/SgNom:      RHyph  ;
             +Ess:n     K-CONS ;
          +Sg+Ill:Q4j   K-CONS ; !Q4 to get e:i
          +Sg+Gen:      K-VOW  ;
       +Cmp/SgGen:      RHyph  ;
          +Sg+Acc:v     K-CONS ;
          +Sg+Ine:n     K-CONS ;
          +Sg+Ela:s     K-CONS ;
          +Pl+Nom:      K-VOW  ;
          +Pl+Gen:Q4j   K-CONS ; !Q4 to get e:i
