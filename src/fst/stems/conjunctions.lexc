ge        Cc-VOW ;
goappátja Cc-VOW ;
ja        Cc-CONS ;
jale      Cc-VOW ;
jali      Cc-VOW ;
jalik     Cc-CONS ;
juobi     Cc-VOW ;
juogu     Cc-VOW ;
masku     Cc-VOW ;
mavas     Cc-CONS ;
!men       Cc-VOW ; !not smj
vaj       Cc-CONS ;
val       Cc-CONS ;
valla     Cc-VOW ;
váj       Cc-CONS ;
vájbi     Cc-VOW ;


LEXICON Cc-CONS
