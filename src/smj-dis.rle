# Gen after Num

SELECT (Sg Gen) IF (*-1 Num BARRIER NPNH)(NOT 0 Po OR CC);
	## Aktat ij buvte guokta härrä dievnnut.

# Gen in Spec
REMOVE (Pl Nom) IF (0 Prop)(*1 N BARRIER NOT-ADJ);

# Gen before Po

SELECT Gen IF (1C Po);
	## Profehta baktu.
	
# Coordination:	
