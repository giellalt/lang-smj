LEXICON COMMA
  ENDLEX       ;
  COMMASECTION ;

LEXICON COMMASECTION
  ,:% tsiehkes%        NUMBERSECTION ;
 %.:% tjuogga%         NUMBERSECTION ; ! these will overlap with dates
 %::% gærddotjuogga%   NUMBERSECTION ;
 %-:% tjanássáhtso%    NUMBERSECTION ;
 %=:% la%              NUMBERSECTION ;
 %°:% gráda%           NUMBERSECTION ;
  §:% paragráffa%      NUMBERSECTION ;
 %*:% násste%          NUMBERSECTION ;
  &:% ja%              NUMBERSECTION ;
  
  
LEXICON ENDLEX
